# Configuración de la API de iNaturalist
# Reemplaza estos valores con tus propias credenciales

# Token de API (preferido)
INATURALIST_API_TOKEN=

# Token JWT (alternativo)
INATURALIST_JWT_TOKEN=

# Habilitar modo de demostración (sin conexión a iNaturalist)
# Útil para desarrollo cuando no se tienen credenciales de API
DEMO_MODE=true

# Configuración de logging
LOG_LEVEL=INFO
